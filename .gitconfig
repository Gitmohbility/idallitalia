[format]
    pretty = format:%C(yellow)%h%Creset %s %Cgreen(%cr)%Creset %C(bold blue)<%an>%Creset
    mailmap = true
    subjectprefix = "git"

[push]
    default = simple
    followTags = true

[branch]
    autosetuprebase = always
    autosetupmerge = true

[init]
    defaultBranch = main

[advice]
    statusHints = false
    pushUnqualifiedRefname = false
    addEmbeddedRepo = warn
    addIgnoredFile = warn
    addOutsideRepository = warn

[credential]
    helper = osxkeychain

